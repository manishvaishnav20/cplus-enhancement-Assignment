//8) write a function which can accept minimum 3 characters and maximum 5 characters and display them. ( use "default arguments")
#include<iostream>
using namespace std;
void disp(char a,char b,char c,char d=' ',char e=' ')
{
	cout<<a<<"\t"<<b<<"\t"<<c<<"\t"<<d<<"\t"<<e<<endl;
}
void main()
{
	disp('a','d','h');
	disp('k','n','q','t');
	disp('w','a','f','j','n');
}
