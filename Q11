/*11)define 3 different structures and initialize their members.
now write 3 different functions,but with the same name.
these functions will accept each structure respectively and display their members.*/
#include<iostream>
using namespace std;
struct a
{
	int num1;
}var1={10};
struct b
{
	int num2;
}var2={20};
struct c
{
	int num3;
}var3={30};
void disp(a &ob1)
{
	cout<<"num1= "<<ob1.num1<<endl;
}
void disp(b &ob2)
{
	cout<<"num2= "<<ob2.num2<<endl;
}
void disp(c &ob3)
{
	cout<<"num3= "<<ob3.num3<<endl;
}
void main()
{
	disp(var1);
	disp(var2);
	disp(var3);
}
